package main;
import warrior.*;
public class Master {
	
	public static void main(String[] args){
		
		Warrior k = new Knight("Jason");
		
		Warrior a = new Archer("John");
		
		a.display();
		k.display();
		
		int[] n , b, c;
		n = b = c = new int[2];
				
			for( int x = 0 ; x < 2; x++){
				n[x] = 3*(x+1);
				b[x]= 17*(x+2);
				c[x]= 122*(x+3);
			}
		int num = 0; 
		for(int value : n){
			System.out.println("n:"+value+" b:"+b[num]+" c:"+c[num]);
			num++;
		}
//		System.out.println("hello world");
	}

}
